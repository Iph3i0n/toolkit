<s:meta name="wisdom-start"></s:meta>

<script>
  import { GetJson } from "../utils/fetch";

  let layouts = undefined;

  $before: {
    layouts = await GetJson("/api/v1/schema/layouts");
  }
</script>

<l-container>
  <l-row>
    <l-col xs="12">
      <d-listgroup>
        <s:for subject=":layouts" key="layout">
          <span>
            <s:text use=":layout" />
          </span>
        </s:for>
      </d-listgroup>
    </l-col>
  </l-row>
</l-container>
