{
  "name": "localstack-explorer",
  "displayName": "Localstack Explorer",
  "description": "A tool to interact with your local stack setup through VsCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "views": {
      "localstack-explorer": [
        {
          "id": "CloudformationStacks",
          "name": "Cloudformation Stacks"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "localstack-explorer",
          "title": "Localstack Explorer",
          "icon": "media/aws.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "tsc --noEmit && node esbuild.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/node": "20.x",
    "esbuild": "^0.24.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.6.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
